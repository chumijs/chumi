{
  "semi": true,
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "trailingComma": "none",
  "printWidth": 100,
  "parser": "babel",
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "**.json",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.scss",
        "*.css"
      ],
      "options": {
        "parser": "css"
      }
    }
  ]
}